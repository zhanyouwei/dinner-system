{
  "name": "kaihei.wang-apidoc",
  "version": "0.1.0",
  "description": "kaihei.wang web API Documentation Generator",
  "author": "Youwei <zhanyouwei@163.com>",
  "bin": "bin/apidoc",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run jshint && mocha test/",
    "jshint": "jshint lib/ test/",
    "build-src": "bin/apidoc -i api-src/ -o tmp-build/"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "dependencies": {
    "apidoc-core": "~0.4.0",
    "fs-extra": "~0.18.2",
    "lodash": "~3.8.0",
    "markdown-it": "^4.2.1",
    "nomnom": "~1.8.1",
    "winston": "~1.0.0"
  },
  "devDependencies": {
    "apidoc-example": "*",
    "jshint": "^2.7.0",
    "mocha": "~2.2.4",
    "npm-check-updates": "^1.5.1",
    "path-to-regexp": "^1.0.3",
    "semver": "^4.3.4",
    "should": "~6.0.1"
  },
  "jshintConfig": {
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "latedef": false,
    "newcap": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "node": true,
    "browser": true,
    "predef": [
      "it",
      "describe",
      "before",
      "after"
    ]
  }
}
